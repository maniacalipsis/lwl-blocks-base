{
   "$schema":"https://schemas.wp.org/trunk/block.json",
   "apiVersion":3,
   "name":"lwl-blocks-base/image-header-string",
   "version":"1.0.0",
   "title":"Image-Header-String",
   "category":"design",
   "icon":"analytics",
   "description":"List of items with image, header and unformatted text",
   "example":{},
   "supports":{
                 "html":false,
                 "anchor":true,
                 "className":{"type":"string","default":"item"}
              },
   "attributes":{
                   "anchor":{"type":"string"},
                   "image":{"type":"object"},
                   "header_tag":{"type":"string","enum":["H2","H3","H4","H5","H6","DIV"],"default":3},
                   "header_text":{"type":"string"},
                   "text":{"type":"string"}
				    },
   "textdomain":"lwl-blocks-base",
   "editorScript":"file:./index.js",
   "editorStyle":"file:./index.css",
   "style":"file:./style-index.css",
   "render":"file:./render.php",
   "viewScript":"file:./view.js"
}
